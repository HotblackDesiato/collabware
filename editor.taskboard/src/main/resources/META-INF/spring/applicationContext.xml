<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:osgi="http://www.springframework.org/schema/osgi"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd
		http://www.springframework.org/schema/osgi http://www.springframework.org/schema/osgi/spring-osgi.xsd"
		default-autowire="autodetect">
	<context:component-scan base-package="collabware.editor" />
	<context:annotation-config />

	<osgi:reference id="editorRegistry" interface="collabware.registry.EditorRegistry" />	
	<bean init-method="startup" destroy-method="shutdown" id="exampleEditor" class="collabware.taskboard.TaskboardEditor" lazy-init="false">
		<property name="editorRegistry" ref="editorRegistry"></property>
	</bean>
</beans>
